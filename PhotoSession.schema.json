{
  "name": "PhotoSession",
  "type": "object",
  "properties": {
    "client_name": { "type": "string", "description": "Name of the client" },
    "client_email": { "type": "string", "format": "email", "description": "Client's email address" },
    "client_phone": { "type": "string", "description": "Client's phone number" },
    "session_type": { "type": "string", "enum": ["sports", "wedding", "family"], "description": "Type of photography session" },
    "package": { "type": "string", "enum": ["basic", "standard", "premium"], "description": "Selected package" },
    "preferred_date": { "type": "string", "format": "date", "description": "Preferred session date" },
    "location": { "type": "string", "description": "Session location or venue" },
    "status": { "type": "string", "enum": ["pending", "confirmed", "completed", "cancelled"], "default": "pending", "description": "Booking status" },
    "notes": { "type": "string", "description": "Additional client notes or requirements" },
    "estimated_price": { "type": "number", "description": "Estimated session price" }
  },
  "required": ["client_name", "client_email", "session_type", "package", "preferred_date"],
  "rls": { "create": "public", "read": "admin", "update": "admin", "delete": "admin" }
}